
@{
    ViewBag.Title = "UserFromFile";
    Layout = "~/Views/Shared/AdminDashboard.cshtml";
}



@if (TempData["Error"] != null)
{
    <div class="alert alert-danger"><span>@TempData["Error"]</span></div>
}
@if (TempData["Success"] != null)
{
    <div class="alert alert-success"><span>@TempData["Success"]</span></div>
}

<span>Import From Excel</span>
@using (Html.BeginForm("UserFromFile", "Admin", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="container" style="margin-top:20pt;">
        <div class="row">


            
                <div>
                    <input type="file" name="UploadedFile" class="form-control col-sm-10" />
                    <br />
                    <input type="submit" value="submit" name="submit" class="btn btn-success" />
                </div>
           

        </div>
    </div>
}
